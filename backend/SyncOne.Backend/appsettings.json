{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=syncone;Username=postgres;Password=yourpassword"
  },
  "MedGemma": {
    "Endpoint": "https://your-model-endpoint.azure.com",
    "ApiKey": "your-api-key"
  },
  "Embeddings": {
    "Endpoint": "https://your-embedding-endpoint.azure.com",
    "ApiKey": "your-api-key"
  },
  "OpenAI": {
    "ModelId": "gpt-4",
    "ApiKey": "your-openai-api-key"
  },
  "FCM": {
    "ServerKey": "your-fcm-server-key"
  },
  "RemoteWipe": {
    "Secret": "your-secret-key-min-32-chars-change-in-production"
  },
  "Cors": {
    "AllowedOrigins": ["http://localhost:8081", "https://your-app.com"]
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/syncone-.log",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
