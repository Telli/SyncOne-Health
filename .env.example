# Database
DB_PASSWORD=your_secure_database_password

# MedGemma-27B Cloud Endpoint
MEDGEMMA_ENDPOINT=https://your-medgemma-endpoint.azure.com
MEDGEMMA_API_KEY=your_medgemma_api_key

# Embeddings Service (BGE-small-en)
EMBEDDINGS_ENDPOINT=https://your-embeddings-endpoint.com
EMBEDDINGS_API_KEY=your_embeddings_api_key

# OpenAI (for Semantic Kernel classification)
OPENAI_API_KEY=sk-your-openai-api-key

# Firebase Cloud Messaging (for alerts)
FCM_SERVER_KEY=your_fcm_server_key

# Remote Wipe Secret (min 32 characters)
REMOTE_WIPE_SECRET=your_random_secret_min_32_chars_change_in_production

# CORS Allowed Origins
ALLOWED_ORIGIN=https://your-app.com

